<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.inventory.mapper.MesWarehouseMapper">
    
    <resultMap type="MesWarehouse" id="MesWarehouseResult">
        <result property="warehouseId"    column="warehouse_id"    />
        <result property="warehouseName"    column="warehouse_name"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectMesWarehouseVo">
        select warehouse_id, warehouse_name, create_by, create_time, update_by, update_time, remark from mes_warehouse
    </sql>

    <select id="selectMesWarehouseList" parameterType="MesWarehouse" resultMap="MesWarehouseResult">
        <include refid="selectMesWarehouseVo"/>
        <where>  
            <if test="warehouseName != null  and warehouseName != ''"> and warehouse_name like concat('%', #{warehouseName}, '%')</if>
        </where>
    </select>
    
    <select id="selectMesWarehouseByWarehouseId" parameterType="Long" resultMap="MesWarehouseResult">
        <include refid="selectMesWarehouseVo"/>
        where warehouse_id = #{warehouseId}
    </select>
        
    <insert id="insertMesWarehouse" parameterType="MesWarehouse" useGeneratedKeys="true" keyProperty="warehouseId">
        insert into mes_warehouse
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="warehouseName != null">warehouse_name,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="warehouseName != null">#{warehouseName},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateMesWarehouse" parameterType="MesWarehouse">
        update mes_warehouse
        <trim prefix="SET" suffixOverrides=",">
            <if test="warehouseName != null">warehouse_name = #{warehouseName},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where warehouse_id = #{warehouseId}
    </update>

    <delete id="deleteMesWarehouseByWarehouseId" parameterType="Long">
        delete from mes_warehouse where warehouse_id = #{warehouseId}
    </delete>

    <delete id="deleteMesWarehouseByWarehouseIds" parameterType="String">
        delete from mes_warehouse where warehouse_id in 
        <foreach item="warehouseId" collection="array" open="(" separator="," close=")">
            #{warehouseId}
        </foreach>
    </delete>
</mapper>